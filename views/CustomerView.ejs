<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8" />
        <meta name="viewport"
            content="width=device-width, initial-scale=1, shrink-to-fit=no" />
        <meta name="description" content />
        <meta name="author" content />
        <title>Gozak</title>
        <!-- Favicon-->
        <link rel="icon" type="image/x-icon" href="assets/favicon.ico" />
        <!-- Bootstrap icons-->
        <link
            href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css"
            rel="stylesheet" />
        <!-- Core theme CSS (includes Bootstrap)-->
        <!-- <link href="../styles.css" rel="stylesheet" /> -->
        <link
            href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css"
            rel="stylesheet"
            integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC"
            crossorigin="anonymous">
    </head>
    <body>
        
        <%- include('partials/navbarButton') %>

        <section class="bg-light py-5 border-buttom">
            <div class="container px-5 my-5">
                <div class="text-center mb-5">
                    <h2 class="fw-bolder" style="color: green;"
                        id="services">Halo Selamat Datang Di Gozak, <span style="color: black;">
                            <%= user.fullName %>
                        </span></h2>
                    <h2 class="fw-bolder mt-5" style="color: green;"
                        id="services">Services By
                        Gozak</h2>
                    <!-- <p class="lead mb-0">With our no hassle pricing plans</p> -->
                </div>
                <div class="row gx-5 gp-6  justify-content-center">
                    <!-- Pricing card free-->
                    <div class="col-lg-6 col-xl-4">
                        <div class="card mb-5 mb-xl-0"
                            style="border-radius: 30px;">
                            <div class="card-body p-5">
                                <div class="mb-3">
                                    <h3 style="color: green;">CarZak</h3>
                                </div>
                                <div>
                                    <img
                                        src="data:image/jpeg;base64,"
                                        alt style="width: 100px;">
                                </div>
                                <p>Layanan Services Mobil</p>
                                <p>Tarif: 10.000/km</p>
                                <p>Pokoknya cepat sampe, DIkawal Polisi</p>
                                <div class="d-grid"><a class="btn btn-success"
                                        href="/customerView/<%= userid %>/formOrderCarzak?id=1&name=CarZak">Order</a></div>
                            </div>
                        </div>
                    </div>
                    <!-- Pricing card pro-->
                    <div class="col-lg-6 col-xl-4">
                        <div class="card mb-5 mb-xl-0"
                            style="border-radius: 30px;">
                            <div class="card-body p-5">
                                <div class="mb-3">
                                    <h3 style="color: green;">RideZak</h3>
                                </div>
                                <div>
                                    <img
                                        src="https://image.made-in-china.com/202f0j00HiEkzGOIIucT/Cheap-Electric-Motorbike-Kids-Battery-Powered-Motorcycle-Baby-Toys-Bike-Kids-Electric-Motorcycle.webp"
                                        alt style="width: 100px;">
                                </div>
                                <p>Layanan Services Motor</p>
                                <p>Tarif: 5.000</p>
                                <p>Pokoknya cepat sampe, Anti rem</p>
                                <div class="d-grid"><a class="btn btn-success"
                                        href="/customerView/<%= userid %>/formOrderRidezak?id=2&name=RideZak">Order</a></div>
                            </div>
                        </div>
                    </div>
                    <!-- Pricing card enterprise-->
                </div>
            </div>
        </section>
    </body>